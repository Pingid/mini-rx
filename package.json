{
  "name": "mini-rx",
  "version": "0.0.0-dev.1",
  "description": "A mini reactive programming library",
  "main": "lib/cjs/index.js",
  "typings": "lib/cjs/index.d.ts",
  "module": "lib/esm/index.js",
  "author": "Dan Beaven <dm.beaven@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pingid/mini-rx"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/cjs/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./dom": {
      "types": "./lib/cjs/dom.d.ts",
      "require": "./lib/cjs/dom.js",
      "import": "./lib/esm/dom.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "rxjs",
    "reactive",
    "fp",
    "functional"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-cjs.json && tsc -p tsconfig-esm.json",
    "test": "vitest"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
